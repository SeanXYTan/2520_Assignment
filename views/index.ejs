<%- layout('layout.ejs') %>
<div>
    <div class="page-header">
        <h1 class="text-info"><%- title %></h1>
    </div>
    <table class="table table-bordered table-condensed">
            <tr class="active">
                <th>Country</th>
                <th>Sales</th>
                <th>Year</th>
            </tr>

        <% for(var i=0; i<jsonData.length; i++) { %>
            <tr>
                <td><%= jsonData[i].Country %></td>
                <td><%= jsonData[i].Sales %></td>
                <td><%= jsonData[i].Year %></td>
            </tr>
        <% } %>
    </table>
</div>